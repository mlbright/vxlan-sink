# Example Terraform variables file
# Copy this to terraform.tfvars and fill in your values

# AWS region where the OIDC provider will be created
aws_region = "us-east-1"

# Your GitHub organization or username
# Example: "mycompany" or "myusername"
github_org = "your-github-org"

# Your GitHub repository name
# Example: "vxlan-ami-builder"
github_repo = "your-repo-name"

# Set to true if you already have a GitHub OIDC provider in your AWS account
# AWS allows only ONE OIDC provider per account for GitHub
# If you're using GitHub Actions OIDC with other repositories, set this to true
oidc_provider_exists = false
